{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": "airbnb-base",
    "rules": {
        "no-console": "warn",
        "quotes": [
            "error",
            "double"
        ],
        "no-restricted-syntax": "off",
        "no-use-before-define": "off"
    },
    "plugins": [
        "html",
        "markdown"
    ]
}